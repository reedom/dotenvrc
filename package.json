{
  "name": "dotenvrc",
  "version": "0.1.0",
  "description": "Read .envrc",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "tsc": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist/*",
    "build": "npm-run-all clean tsc && cp package.json dist/",
    "publish": "cd dist && npm publish",
    "lint": "eslint --ext .ts,.js src/",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --ext .ts,.js",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reedom/dotenvrc"
  },
  "keywords": [
    "direnv",
    "envrc",
    "shell",
    "config",
    "export",
    "environment",
    "variable",
    "sh",
    "posix",
    "bash",
    "zsh"
  ],
  "author": "HANAI, Tohru",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reedom/dotenvrc"
  },
  "homepage": "https://github.com/reedom/dotenvrc/",
  "dependencies": {
    "bash-parser": "^0.5.0"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.7.11",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "husky": "^3.0.8",
    "jest": "^25.1.0",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^3.6.3"
  }
}
